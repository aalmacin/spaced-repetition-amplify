{
  "name": "spaced-repetition-amplify",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4311",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "e2e": "ng e2e",
    "e2e:watch": "ng e2e --watch",
    "format:test": "prettier --config .prettierrc --list-different \"./src/**/*.{ts,js,scss,html}\"",
    "format:fix": "prettier --config .prettierrc --write \"./src/**/*.{ts,js,scss,html}\"",
    "fix": "npm run format:fix; npm run lint:fix"
  },
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && ng lint"
    }
  },
